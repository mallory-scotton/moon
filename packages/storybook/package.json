{
  "name": "@moon/storybook",
  "version": "1.0.0",
  "description": "The Moon Home Theater react components storybook",
  "keywords": [
    "storybook"
  ],
  "type": "module",
  "author": {
    "name": "Mallory SCOTTON",
    "email": "mscotton.pro@gmail.com",
    "url": "https://github.com/mallory-scotton"
  },
  "homepage": "https://github.com/mallory-scotton/moon",
  "license": "MIT",
  "main": "src/index.ts",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mallory-scotton/moon",
    "directory": "packages/storybook"
  },
  "bugs": {
    "url": "https://github.com/mallory-scotton/moon/issues"
  },
  "scripts": {
    "dev": "pnpm storybook dev -p 6006",
    "build": "pnpm storybook build",
    "start": "pnpm dlx http-server storybook-static"
  },
  "tsup": {
    "clean": true,
    "target": "ES2020",
    "format": [
      "cjs",
      "esm"
    ]
  },
  "dependencies": {
    "@moon/theme": "workspace:*",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-docs": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/builder-vite": "^8.4.7",
    "@storybook/cli": "^8.4.7",
    "@storybook/manager-api": "^8.4.7",
    "@storybook/preview-api": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/theming": "^8.4.7",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "remark-gfm": "^4.0.1",
    "storybook": "^8.4.7",
    "storybook-dark-mode": "^4.0.2",
    "tailwindcss": "4.1.11",
    "vite": "^6.0.4"
  }
}
